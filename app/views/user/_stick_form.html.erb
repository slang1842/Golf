<div id="<%= "users_stick_#{users_stick.id}" unless users_stick.new_record? %>"  >  
  <%= f.fields_for users_sticks_attributes, users_stick do |h| %>
    <%= h.hidden_field :id unless users_stick.new_record? %>

    <div class="club_set">
      <div class="item">
        <div class="subitem"><%= f.label "Club type" %></div>
        <div class="subitem"><%= h.label :distance %></div>
        <div class="subitem"><%= h.label :degrees %></div>
        <div class="subitem"><%= h.label :shaft %></div>
        <div class="subitem"><%= h.label :shaft_strength %></div>
      </div>

      <div class="item">
        <% if users_stick.id != nil %>
          <div class="subitem"><%= h.collection_select :stick_id, Stick.all, :id, :stick_type, {:prompt => "Select type"},{:class => "dropdown5", :onchange => "fill_sticks_form(this)", :disabled => "disabled" } %></div>
        <% else %>
          <div class="subitem"><%= h.collection_select :stick_id, Stick.all, :id, :stick_type, {:prompt => "Select type"},{:class => "dropdown5", :onchange => "fill_sticks_form(this)"} %></div>
        <% end %>
        <div class="subitem"><%= h.text_field :distance, :class => 'distance  textbox2' %></div>
        <div class="subitem"><%= h.select(:degrees, STICK_DEGREES,{}, { :class => 'dropdown8 user_select_field degrees'}) %></div>
        <div class="subitem"><%= h.select(:shaft, STICK_SHAFT,{}, { :class => 'dropdown8 user_select_field shaft'}) %></div>
        <div class="subitem"><%= h.select(:shaft_strength, STICK_SHAFT_STRENGTH,{}, { :class => 'dropdown8 user_select_field shaft_strength'}) %></div>
        <div class="subitem2">

          <% if users_stick.new_record? %>
            <a href="" OnClick="$(this).parent().parent().parent().slideUp('fast', function() { $(this).remove(); } ); return false;" class="gray_button">Cancel</a>
          <% end %>

        </div>
      </div>


    </div>

  </div>
<% end %>
