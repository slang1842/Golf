<script type="text/javascript">
$(document).ready(function() {
	swapActiveStick("<%=stick.id%>"); 
  });

</script>

<!-- =-=-=-=-=-=-=-=-=-=-=-=-=-=- Place =-=-=-=-=-=-=-=-=-=-=-=-=-=- -->
<p>Currently showing statistics for <%= stick.stick_type %></p>
<div class="clear"></div>
<div class="situation_type" style="" >
  <span onclick="showHideElement('#place')">
    <div class="name_wrap">
      <p>Place</p>
      <img src="/images/link_right_small.png" alt=""/>
    </div>
    <div class="info_wrap" id="length_stats_div" style="">
      <img src="/images/table_icon_place.png" alt=""/>
      <div class="description">
        <p>Place, where the hit took place</p>
      </div>
    </div>
  </span>
  <div class="single_situation" id="place" >


 <div class="clear"></div>
    <!-- ========================================= place_teebox ========================================= -->
    <div class="name">
      <p>Teebox</p>
    </div>
    <% hint_type = "place_teebox_hint" %>         
    <% stat_type = "place_teebox" %>
<div class="progress_block">
  <div class="tip_bubble_wrap" id="<%= hint_type %><%= statistic.id %>" style="display: none;">
  <div class="triangle"></div>
    <div class="frame">
			<% curr_stats = statistic.send(stat_type)%>
    		<% if is_admin || is_coach %>
      		<% if stats_owner.id != current_user.id %>
      	  	<p><%= "Give hint to (#{stats_owner.first_name} #{stats_owner.last_name}) for this situation." %> </p>
      	  <% else %>
         		<p>Give  hint for this situation.</p>
        	<% end %>
        	<div class="tip">
         	<%= form_for (owner_hint, :as => :hint, :html => { :onsubmit=> ""}, :url => update_hints_statistic_path) do |f|  %>
						<%= f.hidden_field :user_id, :value => @user_params.id %>
						<% if owner_hint.send(hint_type) == "" %>
            	<%= f.text_area hint_type, :class => "textarea", :value => coach_hint.send(hint_type) %>
						<% else %>
							<%= f.text_area hint_type, :class => "textarea" %>
						<% end %>
            <%= f.hidden_field :stick_id %>
            <%= f.submit :class => "button_save" %>
          <% end %>
        </div>
   	   	<% else %>
        	<p><%#= "Coach #{fetch_coach(stats_owner.coach)} hint for this situation."  %></p>
       		<div class="tip">
          	<p><%= owner_hint.send(hint_type) %></p>
        	</div>
      	<% end %>
    	</div>
  	</div>
 	 	<% unless curr_stats == 0 %>
    	<div class="progress_bar">
      	<div class="progress_active <%= swap_bar_color(curr_stats) %>" style="width:<%= "#{curr_stats}%"%>">
      	  <div class="percentage">
     	   		<p><%= curr_stats %>%</p>
        	</div>
      	</div>
    	</div>
 		<% else %>
    	<div class="no_value"><p><%= MAIN_STATISTIC_BAD_SIMBOL %></p></div>
  	<% end %>
	  <% if is_admin || has_couch && owner_hint.send(hint_type) != ""%>
    	<div class="notification" onclick="$('#<%= hint_type %><%= statistic.id %>').toggle();"><img src="/images/tip_icon.png" alt=""/></div>
  	<% end %> 
</div>
          
