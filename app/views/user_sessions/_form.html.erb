<%= form_for user_session, :url => {:action => "create", :controller => "user_sessions"} do |f| %>
  <%=  f.error_messages %>
  <%=  f.label :email %>
  <%=  f.text_field :email %>
  <%=  f.label :password %>
  <%=  f.password_field :password %>
  <%=  f.check_box :remember_me %>
  <%=  f.label :remember_me %>
  <%=  f.submit "Login" %>
<% end %>